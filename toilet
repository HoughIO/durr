# http://sequoia.github.io/blog/fun-with-toilet/
tput setaf 1 && toilet -f smbraille $(whoami);
tput setaf 1 && toilet -f pagga $(whoami);
tput setaf 2 && toilet -f pagga $(whoami);
tput setaf 2 && tput setab 6 && toilet -f future $(whoami);
tput setaf 1 && tput setab 6 && toilet -f future $(whoami);


while read -r filt; do
    echo "$filt";
    toilet -f mono12 $USER -F "$filt";
done < <(toilet -F list | sed -n 's/\"\(.*\)\".*/\1/p')

# Work well with Campfire
